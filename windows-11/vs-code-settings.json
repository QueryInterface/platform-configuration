{
    "workbench.tree.indent": 15,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.colorCustomizations": {
        "tree.indentGuidesStroke": "#00ff00"
    },
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "workbench.editorAssociations": {
        "*.svg": "default"
    },
    "git.confirmSync": false,
    "cmake.configureOnOpen": false,
    "cmake.useCMakePresets": "auto",
    "cmake.buildDirectory": "${workspaceRoot}/_build/vs",
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "editor.accessibilitySupport": "off",
    "cmake.configureOnEdit": false,
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "vim.handleKeys": {
        "<C-p>": false
    },
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liveshare.languages.allowGuestCommandControl": true,
    "liveshare.openSharedServers": false,
    "liveshare.publishWorkspaceInfo": true,
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "Default Dark+",
    "editor.lineNumbers": "relative",
    "http.proxy": "http://proxy-dmz.intel.com:912",
    "githubPullRequests.pullBranch": "never",
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] },
        { "before": [ "<C-h>" ], "after": [ "<C-w>", "h" ] },
        { "before": [ "<C-j>" ], "after": [ "<C-w>", "j" ] },
        { "before": [ "<C-k>" ], "after": [ "<C-w>", "k" ] },
        { "before": [ "<C-l>" ], "after": [ "<C-w>", "l" ] },
        { "before": [ "<A-,>" ], "commands": [ "workbench.action.navigateBack" ] },
        { "before": [ "<A-.>" ], "commands": [ "workbench.action.navigateForward" ] },
        { "before": [ "g", "r" ], "commands": [ "references-view.findReferences" ] },
        { "before": [ "<Leader>", "t", "t" ], "commands": [ ":tabnew" ] },
        { "before": [ "<Leader>", "t", "n" ], "commands": [ ":tabnext" ] },
        { "before": [ "<Leader>", "t", "p" ], "commands": [ ":tabprev" ] },
        { "before": [ "<Leader>", "t", "o" ], "commands": [ ":tabo" ] },
        { "before": [ "<Leader>", "o" ], "after": [ "o", "<Esc>" ] },
        { "before": [ "<Leader>", "O" ], "after": [ "O", "<Esc>" ] },
        { "before": [ "<Leader>", "j" ], "after": [ "J" ] },
        { "before": [ "<Leader>", "'" ], "commands": [ ":noh" ] },
        { "before": [ "<Leader>", "w" ], "commands": [ "workbench.action.files.save" ] },
        { "before": [ "<Leader>", "f" ], "commands": [ "editor.action.formatDocument" ] },
        { "before": [ "<Leader>", "p" ], "commands": [ "workbench.action.showCommands" ] },
        { "before": [ "<Leader>", "g" ], "commands": [ "workbench.action.gotoSymbol" ] },
        { "before": [ "<Leader>", "/" ], "commands": [ "editor.action.commentLine" ] },
        { "before": [ "<Leader>", "q" ], "commands": [ "openInIntegratedTerminal" ] },
        { "before": [ "<Leader>", "`" ], "commands": [ "workbench.action.terminal.toggleTerminal" ] },
        { "before": [ "<Leader>", "e" ], "commands": [ "workbench.view.explorer" ] },
        { "before": [ "<Leader>", "r" ], "commands": [ "editor.action.refactor" ] },
        { "before": [ "<Leader>", "1" ], "commands": [ "workbench.action.focusFirstEditorGroup" ] },
        { "before": [ "<Leader>", "2" ], "commands": [ "workbench.action.focusSecondEditorGroup" ] },
        { "before": [ "<Leader>", "r" ], "commands": [ "editor.action.rename" ] },
        { "before": [ "<Leader>", "h" ], "commands": [ "workbench.action.quickOpen" ] },
        { "before": [ "<Leader>", "<Leader>", "z" ], "commands": [ "workbench.panel.chat.view.copilot.focus" ] },
        { "before": [ "<Leader>", "n" ], "commands": [ "editor.action.marker.nextInFiles.error" ] },
        { "before": [ "<Leader>", "N" ], "commands": [ "editor.action.marker.prevInFiles.error" ] }
    ],
    "vim.sneak": true,
    "files.autoSave": "afterDelay",
    "[cmake]": {
        "editor.defaultFormatter": "cheshirekow.cmake-format"
    },
    "git.allowForcePush": true,
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Python Debugger: Current File with Arguments",
                "type": "debugpy",
                "request": "launch",
                "program": "${file}",
                "console": "integratedTerminal",
                "args": "${command:pickArgs}"
            }
        ]
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "workbench.iconTheme": "vscode-icons",
    "cSpell.userWords": [
        "endfunction"
    ],
    "cmake.default.buildDirectory": "${workspaceFolder}/_build/vscode",
    "git.enableSmartCommit": true,
    "[groovy]": {
        "editor.defaultFormatter": "NicolasVuillamy.vscode-groovy-lint"
    }
}